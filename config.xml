<?xml version='1.0' encoding='utf-8'?>
<widget id="com.notification.bistroux" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>BistroUX</name>
    <description>
    BistroUX
  </description>
    <author email="support@cybervationinc.com" href="http://cybervationinc.com">
    Cybervation, Inc.
  </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <preference name="android-minSdkVersion" value="14" />
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="BackupWebStorage" value="none" />
    </platform>
    <preference name="DisallowOverscroll" value="true" />
    <preference name="orientation" value="portrait" />
    <preference name="fullscreen" value="true" />
    <preference name="KeepRunning" value="false" />
    <preference name="SplashScreen" value="screen" />
    <preference name="android-build-tool" value="gradle" />
    <plugin name="cordova-plugin-device" source="npm" spec="1.0.1" />
    <plugin name="cordova-plugin-media" source="npm" spec="1.0.1" />
    <plugin name="cordova-plugin-network-information" source="npm" spec="1.0.1" />
    <plugin name="cordova-plugin-whitelist" source="npm" spec="1.1.0" />
    <plugin name="cordova-plugin-whitelist" spec="~1.2.0" />
    <plugin name="cordova-plugin-console" spec="~1.0.1" />
    <plugin name="cordova-plugin-statusbar" spec="~1.0.1" />
    <plugin name="cordova-plugin-whitelist" source="npm" />
    <plugin name="cordova-plugin-x-socialsharing" source="npm" />
    <plugin name="cordova-plugin-inappbrowser" source="npm" />
    <plugin source="git" spec="https://github.com/floatinghotpot/cordova-admob-pro" />
    <plugin name="cordova-plugin-network-information" source="npm" />
    <plugin name="cordova-universal-clipboard" source="npm" />
    <plugin name="phonegap-plugin-barcodescanner" source="npm" />
    <plugin name="cordova-plugin-x-toast" />
    <engine name="android" spec="~7.1.4" />
</widget>
